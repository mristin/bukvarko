{
  "name": "bukvarko",
  "version": "1.0.0",
  "description": "Teach kids alphabet in a fun way.",
  "private": true,
  "scripts": {
    "format": "prettier --write 'src/**/*.{ts,tsx}'",
    "check": "prettier --check 'src/**/*.{ts,tsx}' && tsc --noEmit",
    "start": "parcel src/index.html",
    "test": "jest",
    "build": "parcel build src/index.html"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mristin/bukvarko.git"
  },
  "keywords": [
    "teach",
    "kids",
    "learn",
    "alphabet",
    "game"
  ],
  "author": "Marko Ristin",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mristin/bukvarko/issues"
  },
  "homepage": "https://github.com/mristin/bukvarko#readme",
  "importSort": {
    ".js, .jsx, .ts, .tsx": {
      "style": "module",
      "parser": "typescript"
    }
  },
  "jest": {
    "preset": "ts-jest",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "testMatch": [
      "**/__tests__/**/*.(test|spec).(ts|tsx|js)"
    ]
  },
  "devDependencies": {
    "@testing-library/react": "^10.0.2",
    "@types/jest": "^25.1.4",
    "@types/testing-library__react": "^10.0.1",
    "import-sort-style-module": "^6.0.0",
    "jest": "^25.2.4",
    "parcel-bundler": "^1.12.4",
    "prettier": "^2.0.1",
    "prettier-plugin-import-sort": "0.0.4",
    "source-map-loader": "^0.2.4",
    "ts-jest": "^25.3.0",
    "ts-loader": "^6.2.2",
    "typescript": "^3.8.3"
  },
  "dependencies": {
    "@material-ui/core": "^4.9.7",
    "@material-ui/icons": "^4.9.1",
    "@types/deep-equal": "^1.0.1",
    "@types/react": "^16.9.25",
    "@types/react-dom": "^16.9.5",
    "@types/react-redux": "^7.1.7",
    "deep-equal": "^2.0.2",
    "immer": "^6.0.2",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-redux": "^7.2.0",
    "redux": "^4.0.5",
    "redux-persist": "^6.0.0",
    "style.css": "^1.0.0",
    "typeface-roboto": "0.0.75"
  }
}
